+    my ($name, $rrequest) = @_;
+    open my $in, '<', $rrequest;
+    my $first = <$in>;
+    if (!$first) {
+        Test::More::BAIL_OUT("$name - Request line should be non-empty");
+        die;
+    }
+    $first =~ s/^\s+|\s+$//g;
+    my ($meth, $rel_url) = split /\s+/, $first, 2;
+    my $url = "http://localhost:$ServerPortForClient" . $rel_url;
+
+    my $content = do { local $/; <$in> };
+    if ($content) {
+        $content =~ s/^\s+|\s+$//s;
+    }
+
+    close $in;
+
+    return {
+        method  => $meth,
+        url     => $url,
+        content => $content,
+    };
