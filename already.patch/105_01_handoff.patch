+static ngx_int_t 
+ngx_tcp_check_ssl_hello_parse(ngx_tcp_check_peer_conf_t *peer_conf) 
+{
+    size_t                        size;
+    server_ssl_hello_t           *resp;
+    ngx_tcp_check_ctx            *ctx;
+
+    ctx = peer_conf->check_data;
+
+    size = ctx->recv.last - ctx->recv.pos;
+    if (size < sizeof(server_ssl_hello_t)) {
