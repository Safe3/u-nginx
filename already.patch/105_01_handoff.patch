+static ngx_int_t 
+ngx_tcp_check_need_exit() 
+{
+    if (ngx_terminate || ngx_exiting || ngx_quit) {
+        ngx_tcp_check_clear_all_events();
+        return 1;
+    }
+
+    return 0;
+}
