+#if (NGX_TCP_SSL)
+
+ngx_int_t
+ngx_tcp_upstream_set_round_robin_peer_session(ngx_peer_connection_t *pc,
+    void *data)
+{
+    ngx_tcp_upstream_rr_peer_data_t  *rrp = data;
