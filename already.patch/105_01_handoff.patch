+
+    { NGX_TCP_CHECK_HTTP,
+      "http",
+      ngx_string("GET / HTTP/1.0\r\n\r\n"),
+      NGX_CONF_BITMASK_SET | NGX_CHECK_HTTP_2XX | NGX_CHECK_HTTP_3XX,
+      ngx_tcp_check_send_handler,
