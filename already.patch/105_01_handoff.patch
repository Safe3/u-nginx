+static char *ngx_tcp_upstream_ip_hash(ngx_conf_t *cf, ngx_command_t *cmd,
+    void *conf);
+
+
+static ngx_command_t  ngx_tcp_upstream_ip_hash_commands[] = {
+
+    { ngx_string("ip_hash"),
+      NGX_TCP_UPS_CONF|NGX_CONF_NOARGS,
+      ngx_tcp_upstream_ip_hash,
+      0,
+      0,
